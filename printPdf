function printPdf(){

  var file = nlapiPrintRecord('TRANSACTION', 17034, 'PDF', null); //internal id of transaction (e.g Item Fulfillment)
  file.setFolder(6908);
  nlapiSubmitFile(file);
  var x = 1;

}
